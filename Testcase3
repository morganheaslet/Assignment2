[*Find largest value between 3 numbers*]
int num1, num2,num3, largest$
if(num1 > num2)
{
	largest = num1$;
}
 if(num1 > num3)
{
	largest = num1$;
}
if (num2 > num1)
{
  largest = num2
 }
 if (num2 > num3)
{
  largest = num2
 }
 if (num3 > num1)
{
  largest = num3
 }
 if (num3 > num2)
{
  largest = num3
 }
